{
  "name": "fast-text-table",
  "version": "1.0.1",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "bench": "tsx --expose-gc --allow-natives-syntax src/index.bench.ts",
    "build": "rollup -c"
  },
  "sideEffects": false,
  "author": "D-Sketon",
  "license": "MIT",
  "description": "Quickly generate borderless text tables with alignment",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/cli-color": "^2.0.6",
    "@types/text-table": "^0.2.5",
    "cli-color": "^2.0.4",
    "mitata": "^1.0.34",
    "rollup": "^4.41.1",
    "text-table": "^0.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D-Sketon/fast-text-table.git"
  },
  "bugs": {
    "url": "https://github.com/D-Sketon/fast-text-table/issues"
  },
  "homepage": "https://github.com/D-Sketon/fast-text-table#readme"
}
